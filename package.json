    "@storybook/html-vite": "^8.6.3",
    "@swc/core": "^1.9.3",
    "@tailwindcss/vite": "^4.0.0-beta.2",
    "@thg-altitude/standalone-components-css": "latest",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "astro": "^4.16.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lottie-react": "^2.4.1",
