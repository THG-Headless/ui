    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@thg-altitude/standalone-components-css": "file:./standalone-components-css",
    "ajv": "^8.12.0",
    "axe-playwright": "^2.0.3",
    "color-space": "^2.3.2",
    "delta-e": "^0.0.8",
    "gray-matter": "^4.0.3",
    "postcss-import": "^16.1.0",
