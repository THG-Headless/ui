    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rollup": "^4.28.0",
    "shadcn": "^2.6.0-canary.2",
    "shiki": "^1.29.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
