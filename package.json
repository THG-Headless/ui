{
  "name": "astro-starlight",
  "type": "module",
  "version": "0.0.2-alpha.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "build:polyfill": "rollup -c rollup.polyfill.config.js",
    "build:css": "cd standalone-components-css && npm run build",
    "build:all": "npm run build:polyfill && npm run build:css && astro build",
    "publish:css": "npm run build:css && cd standalone-components-css && npm publish --access public",
    "registry:generate": "node scripts/createRegistry/index.js",
    "registry:generate:defaults": "node scripts/createRegistry/index.js --apply-defaults",
    "registry:build": "shadcn registry:build",
    "registry:validate": "node scripts/test-schema.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "test-storybook": "test-storybook",
    "clean": "rm -rf dist .astro public/oklch-polyfill.min.js standalone-components-css/dist",
    "clean:build": "npm run clean && npm run build"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/starlight": "^0.29.2",
    "@fontsource-variable/inter": "^5.1.0",
